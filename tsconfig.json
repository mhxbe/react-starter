{
  "compilerOptions": {
    "outDir": "./dist/",
    "rootDir": ".",
    "strict": true,
    "module": "CommonJS",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "sourceMap": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "lib": ["DOM", "DOM.Iterable", "ESNext", "WebWorker"],
    "noEmit": false,
    "skipLibCheck": true
  },
  "include": ["src", "typings", "service-worker.ts"],
  "exclude": ["node_modules", "dist"]
}
